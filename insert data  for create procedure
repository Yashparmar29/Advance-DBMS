create database employee;
use employee;
create table emp(id int(10)primary key,ename varchar(20),dept varchar(20));#create procedure insert data
delimiter //
create procedure insert_data_emp1(in eid int,empname varchar(20),dep varchar(20))
begin
	insert into emp values(eid,empname,dep);
end //
delimiter ;
call insert_data_emp1(02,'bhavik parmar','developer');
select * from emp;
